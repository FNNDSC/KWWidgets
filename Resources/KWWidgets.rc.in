        BEGIN
            VALUE "FileDescription", "@KWWIDGETS_RES_APP_NAME@\0"
            VALUE "OriginalFilename", "@KWWIDGETS_RES_FILE_NAME@.exe\0"
            VALUE "CompanyName", "@KWWIDGETS_RES_COMPANY_NAME@\0"
            VALUE "FileVersion", "@KWWIDGETS_RES_MAJOR_VERSION@.@KWWIDGETS_RES_MINOR_VERSION@\0"
            VALUE "LegalCopyright", "Copyright \251 2005 by @KWWIDGETS_RES_COMPANY_NAME@\0"
            VALUE "ProductName", "@KWWIDGETS_RES_APP_NAME@ @KWWIDGETS_RES_MAJOR_VERSION@.@KWWIDGETS_RES_MINOR_VERSION@ for Windows\0"
            VALUE "ProductVersion", "@KWWIDGETS_RES_MAJOR_VERSION@.@KWWIDGETS_RES_MINOR_VERSION@\0"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
