#include "Resources/KWWidgets.rc.h"

// The icon whose name or resource ID is lexigraphically first, is used
// as the application's icon.
// Tk was defining app.

IDI_APPLICATION       ICON    DISCARDABLE     "Resources/KWWidgetsIcon32.ico"
IDI_KWWIDGETSICO      ICON    DISCARDABLE     "Resources/KWWidgetsIcon32.ico"
IDI_KWWIDGETSICOSMALL ICON    DISCARDABLE     "Resources/KWWidgetsIcon16.ico"

#include <windows.h>

#include "vtkConfigure.h"
#ifdef VTK_TCL_TK_STATIC
#include "tk_base.rc"
#endif

VS_VERSION_INFO	VERSIONINFO
