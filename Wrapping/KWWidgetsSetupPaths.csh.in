setenv PATH "${KWWidgets_PATH_ENV}:$PATH"
if ( $?TCLLIBPATH ) then
  setenv TCLLIBPATH '${KWWidgets_TCLLIBPATH_ENV} '$TCLLIBPATH
else
  setenv TCLLIBPATH '${KWWidgets_TCLLIBPATH_ENV}'
endif
if ( $?PYTHONPATH ) then
  setenv PYTHONPATH "${KWWidgets_PYTHONPATH_ENV}${KWWidgets_PYTHONPATH_SEP}$PYTHONPATH"
else
  setenv PYTHONPATH "${KWWidgets_PYTHONPATH_ENV}"
endif
