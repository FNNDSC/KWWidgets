setenv PATH "${KWWidgets_PATH_ENV}${KWWidgets_SEP}$PATH"
setenv LD_LIBRARY_PATH "${KWWidgets_PATH_ENV}${KWWidgets_SEP}$LD_LIBRARY_PATH"
setenv DYLD_LIBRARY_PATH "${KWWidgets_PATH_ENV}${KWWidgets_SEP}$DYLD_LIBRARY_PATH"
if ( $?TCLLIBPATH ) then
  setenv TCLLIBPATH '${KWWidgets_TCLLIBPATH_ENV} '$TCLLIBPATH
else
  setenv TCLLIBPATH '${KWWidgets_TCLLIBPATH_ENV}'
endif
if ( $?PYTHONPATH ) then
  setenv PYTHONPATH "${KWWidgets_PYTHONPATH_ENV}${KWWidgets_PYTHONPATH_SEP}$PYTHONPATH"
else
  setenv PYTHONPATH "${KWWidgets_PYTHONPATH_ENV}"
endif
